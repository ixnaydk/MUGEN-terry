;Definição - Short/Long Jump   |
;------------------------------|
[Statedef 40]
type = S
physics = S
anim = 40
ctrl = 0
sprpriority = 1
velset= 0,0

[State 40, VarSet]
type = VarSet
trigger1 = Time = 0
sysvar(1) = 0

[State 40, VarSet]
type = VarSet
trigger1 = command = "holdfwd"
sysvar(1) = 1

[State 40, VarSet]
type = VarSet
trigger1 = command = "holdback"
sysvar(1) = -1

[State 40, VelSet]
type = VelSet
trigger1 = AnimTime = 0
x = ifelse(sysvar(1)=0,const(velocity.jump.neu.x),ifelse(sysvar(1)=1,ifelse(var(6)= 0,const(velocity.jump.fwd.x),4.2),ifelse(var(6)= 0,const(velocity.jump.back.x),-4.2)))
